<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
	<bean id="StudentDetails" class="com.bridgeit.Annotations.StudentDetails">
		<property name="id" value="88"></property>
		<property name="name" value="zeeshan"></property>
	</bean>

	<bean id="studentAddress" class="com.bridgeit.Annotations.StudentAddress">
		<property name="location" value="Banglore"></property>
	</bean>
	
	<bean id="studentAddress2" class="com.bridgeit.Annotations.StudentAddress">
		<property name="location" value="Hyderabad"></property>
	</bean>
</beans>